//carregar IPL
 
:IPLControlLifeInvader_
PushString "facelobby"
CallNative "IS_IPL_ACTIVE" 1 1
push_1
JumpEQ @UnloadLifeInvader_
PushString "facelobby"
CallNative "REQUEST_IPL" 1 0
PushString "facelobbyfake"
CallNative "REMOVE_IPL" 1 0
PushString "~b~Info:~s~ Life Invader loaded."
PushS 2500
Call @PrintText_
Jump @EndIPLControl_
 
:UnloadLifeInvader_
PushString "facelobby"
CallNative "REMOVE_IPL" 1 0
PushString "facelobbyfake"
CallNative "REQUEST_IPL" 1 0
PushString "~b~Info:~s~ Life Invader unloaded."
PushS 2500
Call @PrintText_
Jump @EndIPLControl_
 
 
 
//Shrink
:SelfOptionsShrink_
CallNative "PLAYER_PED_ID" 0 1
Push 223
push_1
CallNative "GET_PED_CONFIG_FLAG" 3 1
push_1
JumpEQ @TurnOffShrink_
CallNative "PLAYER_PED_ID" 0 1
Push 223
push_1
CallNative "SET_PED_CONFIG_FLAG" 3 0
Jump @SelfOptionsReturn_
:TurnOffShrink_
CallNative "PLAYER_PED_ID" 0 1
Push 223
push_0
CallNative "SET_PED_CONFIG_FLAG" 3 0
Jump @SelfOptionsReturn_
 
 
